Time(timer);\n\n  return {\n    startTimer,\n    stopTimer,\n    showCurrentTime,\n  };\n};\n","/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/Signup/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/Signup/Signup.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/components/Modal/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/components/Modal/Modal.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/CurrentSessions/CurrentSessions.js",["139"],"import React, { useContext, useEffect, useState } from \"react\";\nimport { sessions as sessionsAPI } from \"../../api\";\nimport SingleTimer from \"../../components/SingleTimer\";\nimport { AuthContext } from \"../../context/auth/auth\";\n\nexport function CurrentSessions() {\n  const [sessions, setSessions] = useState([]);\n\n  const { user } = useContext(AuthContext);\n\n  useEffect(() => {\n    fetchSessions();\n  }, []);\n\n  const fetchSessions = () => {\n    sessionsAPI.getActive({ userID: user.userID }).then((result) => {\n      setSessions(result);\n    });\n  };\n\n  return (\n    <>\n      {sessions.length > 0 &&\n        sessions.map((session, index) => {\n          return (\n            <SingleTimer\n              key={index}\n              session={session}\n              fetchSessions={fetchSessions}\n            ></SingleTimer>\n          );\n        })}\n      {/* Posibility to track more than 1 session at the same time in the future*/}\n      {sessions.length === 0 ? (\n        <SingleTimer fetchSessions={fetchSessions}></SingleTimer>\n      ) : null}\n    </>\n  );\n}\n","/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/components/SingleTimer/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/components/SingleTimer/SingleTimer.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/utils/msToTime.js",[],"/Users/fernan