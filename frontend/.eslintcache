d/src/components/App/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/components/App/App.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/api/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/api/sessions.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/api/users.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/api/base.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/Login/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/Login/Login.js",["139"],"import React, { useContext, useState } from \"react\";\nimport { Link, Redirect } from \"react-router-dom\";\nimport { users as usersApi } from \"../../api\";\nimport { AuthContext } from \"../../context/auth/auth\";\n\nexport const Login = () => {\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [error, setError] = useState(\"\");\n  const [redirect, setRedirect] = useState(false);\n\n  const { login, user } = useContext(AuthContext);\n\n  const handleForSubmit = (event) => {\n    event.preventDefault();\n    const user = { email: email, password: password };\n    usersApi\n      .login(user)\n      .then((response) => {\n        login(response);\n        setRedirect(true);\n      })\n      .catch((err) => {\n        setError(\"Email or password is not correct\");\n      });\n  };\n\n  if (redirect) {\n    return <Redirect to=\"/current-sessions\" />;\n  }\n\n  return (\n    <div className={\"\"}>\n      <form onSubmit={(e) => handleForSubmit(e)}>\n        <p>Email</p>\n        <input\n          required\n          variant=\"outlined\"\n          size=\"small\"\n          type=\"email\"\n          placeholder=\"email\"\n          value={email}\n          onChange={(newValue) => setEmail(newValue.target.value)}\n        ></input>\n        <p>Password</p>\n        <input\n          required\n          type=\"password\"\n          placeholder=\"*********\"\n          value={password}\n          onChange={(newValue) => setPassword(newValue.target.value)}\n        ></input>\n        <button type=\"submit\">ENTER</button>\n        <p>{error}</p>\n      </form>\n      <Link to=\"/signup\">Or Sign Up</Link>\n    </div>\n  );\n};\n","/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/Not-Found/index.js",[],["140","141"],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/Not-Found/NotFound.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/hooks/useTimer.js",["142"],"import { useContext, useEffect, useState } from \"react\";\nimport { sessions as sessionsAPI } from \"../api\";\nimport { AuthContext } from \"../context/auth/auth\";\nimport msToTime from \"../utils/msToTime\";\n\nexport const useTimer = (startTime = null) => {\n  const [timer, setTimer] = useState(null);\n\n  const { user } = useContext(AuthContext);\n\n  useEffect(() => {\n    if (startTime !== null) {\n      setInterval(() => {\n        setTimer(new Date() - new Date(startTime));\n      }, 1000);\n    } else {\n      setTimer(\"00:00:00\");\n    }\n  }, []);\n\n  const startTimer = async (description) => {\n    const params = {\n      description,\n      startTime: new Date(),\n      userID: user.userID,\n    };\n    await sessionsAPI.startSession(params).then((result) => {\n      return result;\n    });\n  };\n\n  const stopTimer = async (sessionID, endTime) => {\n    const params = {\n      endTime,\n    };\n    await sessionsAPI\n      .endSession(params, sessionID)\n      .then((result) => {\n        return result;\n      })\n      .catch((err) => console.log(err));\n  };\n  \n  const showCurrentTime = () =>\n    startTime === null ? \"00:00:00\" : msToTime(timer);\n\n  return {\n    startTimer,\n    stopTimer,\n    showCurrentTime,\n  };\n};\n","/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/Signup/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/Signup/Signup.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/components/Modal/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/components/Modal/Modal.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/CurrentSessions/CurrentSessions.js",["143"],"import React, { useContext, useEffect, useState } from \"react\";\nimport { sessions as sessionsAPI } from \"../../api\";\nimport SingleTimer from \"../../components/SingleTimer\";\nimport { AuthContext } from \"../../context/auth/auth\";\n\nexport function CurrentSessions() {\n  const [sessions, setSessions] = useState([]);\n\n  const { user } = useContext(AuthContext);\n\n  useEffect(() => {\n    fetchSessions();\n  }, []);\n\n  const fetchSessions = () => {\n    sessionsAPI.getActive({ userID: user.userID }).then((result) => {\n      setSessions(result);\n    });\n  };\n\n  return (\n    <>\n      {sessions.length > 0 &&\n        sessions.map((session, index) => {\n          return (\n            <SingleTimer\n              key={index}\n              session={session}\n              fetchSessions={fetchSessions}\n            ></SingleTimer>\n          );\n        })}\n      {/* Posibility to track more than 1 session at the same time in the future*/}\n      {sessions.length === 0 ? (\n        <SingleTimer fetchSessions={fetchSessions}></SingleTimer>\n      ) : null}\n    </>\n  );\n}\n","/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/components/SingleTimer/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/components/SingleTimer/SingleTimer.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/utils/msToTime.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/FinishedSessions/index.js",[],"/Users/fernando/Documents/Code/tech-challenge-time/frontend/src/pages/FinishedSession